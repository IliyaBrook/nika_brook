  "scripts": {
    "build-dev": "env-cmd -f ./env_files/.env.development next build",
    "start-dev": "env-cmd -f ./env_files/.env.development next dev",
    "build-prod": "env-cmd -f ./env_files/.env.production next build",
    "start-prod": "env-cmd -f ./env_files/.env.production next start",
    "build-local": "env-cmd -f ./env_files/.env.local_prod next build",
    "start-local": "env-cmd -f ./env_files/.env.local_prod next start",
    "start_build_dev": "yarn build-local && env-cmd -f ./env_files/.env.development cross-env NEXT_PUBLIC_LOCATION=dev next start -p 5000",
